{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/typings": true,
        "**/.editorconfig": true,
        "**/.eslintrc.json": true,
        "**/.prettierrc.js": true,
        "**/node_modules": true,
        "**/run": true,
        "**/logs": true,
        "**/package-lock.json": true
    }
}
